  <main class="d-flex flex-nowrap">
    <%- include('partials/menu') %>

      <div class="  b-example-vr"></div>
      <div class="container">
        <div class="container">
          <h1 style="margin-left: 250px;">Simple Inventory</h1>
        </div>


        <div class="container">
          <button type="button" class="btn btn-info" style="margin-right: 20px"><i class="bi bi-file-earmark-plus"
              style="margin-right: 10px;"></i>Add Data</button>
          <button type="button" class="btn btn-info" style="margin-right: 20px"><i class="bi bi-trash3"
              style="margin-right: 10px;"></i>Delete Data</button>
          <button type="button" class="btn btn-info" style="margin-right: 20px"><i class="bi bi-pencil-square"
              style="margin-right: 10px;"></i>Edit Data</button>
        </div>
        <br>
        <table class="table table-hover table-striped">
          <thead class="table-primary">
            <tr>
              <th scope="col">Kode</th>
              <th scope="col">Nama</th>
              <th scope="col">Varian</th>
              <th scope="col">Satuan</th>
              <th scope="col">Jumlah</th>
              <th scope="col">Harga Barang</th>
              <th scope="col">Tanggal Masuk</th>
              <th scope="col">Tanggal Keluar</th>
              <th scope="col">Nama Supplier</th>
            </tr>
          </thead>
          <tbody>
            <% rows.forEach((item)=> { %>
              <tr>
                <th scope="row">
                  <%= item.kode_barang %>
                </th>
                <td>
                  <%= item.nama_barang %>
                </td>
                <td>
                  <%= item.varian_barang %>
                </td>
                <td>
                  <%= item.satuan_barang %>
                </td>
                <td>
                  <%= item.jumlah_barang %>
                </td>
                <td>
                  <%= item.harga_barang %>
                </td>
                <td>
                  <%= moment(item.tanggal_masuk).format('DD MMMM YYYY') %>
                </td>
                <td>
                  <%= moment(item.tanggal_keluar).format('DD MMMM YYYY') %>
                </td>
                <td>
                  <%= item.nama_supplier %>
                </td>
              </tr>
              <% }) %>
          </tbody>
        </table>
      </div>
      </div>
  </main>

  <%- include('partials/script-footer') %>
