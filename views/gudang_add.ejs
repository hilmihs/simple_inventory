    <main class="d-flex flex-nowrap" style="overflow: scroll;">

        <%- include('partials/menu') %>

            <div class="  b-example-vr"></div>
            <div class="container">
                <div class="container">
                    <h1 style="margin-left: 250px;">Simple Inventory</h1>
                </div>

                <h2>Add Gudang</h2>
                <form action="" method="post">
                    <div class="row">
                        <div class="col-25">
                            <label for="id">Kode Gudang</label>
                        </div>
                        <div class="col-75">
                            <input type="text" id="id" name="id" placeholder="ID" value="" required><br>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-25">
                            <label for="nama">Nama Gudang</label>
                        </div>
                        <div class="col-75">
                            <input type="text" id="nama" name="nama" placeholder="Nama" value="" required><br>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-25">
                            <label for="alamat">Alamat</label>
                        </div>
                        <div class="col-75">
                            <input type="text" id="alamat" name="alamat" placeholder="Alamat" value="" required><br>
                        </div>
                    </div>

                    <div class="container" style="margin-top: 25px;">

                        <button type="submit" class="btn btn-success" style="margin-right: 20px"><i class="bi bi-save"
                                style="margin-right: 10px;"></i>Submit</button>
                </form>
                <a href="/gudang"><button type="button" class="btn btn-info"
                        style="margin-right: 20px; float: right; clear: none;"><i class="bi bi-arrow-up-left-square"
                            style="margin-right: 10px;"></i>Return to Gudang</button></a>

            </div>
            </div>
            </div>
    </main>

