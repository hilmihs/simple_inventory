  <main class="d-flex flex-nowrap">
    <%- include('partials/menu') %>

    <div class="  b-example-vr"></div>
    <div class="container" style="overflow-y: scroll;">
      <div class="container">
        <h1 style="margin-left: 250px;">Simple Inventory</h1>
      </div>
      <div class="container" style="float: left;
clear: none;">
        <div class="card"
          style="width: 20%; padding: 2%; margin-bottom: 1%;  background-color: turquoise;float: left; clear: none;">
          <h6 style="margin-bottom: 5%; right:8%; bottom: 8%; position:relative; color:white">Total Barang Gudang</h6>
          <h6 style="left:15%; top: 30%; position:relative; color:white;"><i class="bi bi-box-seam fa-4x"> </i>150</h6>
          </i>
        </div>
        <div class="card"
          style="width: 20%; padding: 2%; margin-bottom: 1%; left:3%; bottom:24.5%; position:relative; background-color: rgb(219, 235, 3); float: left; clear: none;">
          <h6 style="margin-bottom: 5%; right:8%; bottom: 8%; position:relative; color:white">Total Barang Masuk</h6>
          <h6 style="left:15%; top: 30%; position:relative; color:white;"><i
              class="bi bi-box-arrow-in-down fa-4x"></i>150</h6></i>
        </div>
        <div class="card"
          style="width: 20%; padding: 2%; margin-bottom: 1%; margin-left: 3%; left:3%; bottom:24.5%; position:relative; background-color: yellowgreen;float: left; clear: none;">
          <h6 style="margin-bottom: 5%; right:8%; bottom: 8%; position:relative; color:white">Total Barang Keluar </h6>
          <h6 style="left:15%; top: 30%; position:relative; color:white;"><i class="bi bi-box-arrow-in-up fa-4x"></i>
            150</h6></i>
        </div>
      </div>
      <br>
      <br>
      <div class="container" style="margin: 1px;">
        <p style="color: blue; margin-top: 30px;">Transaksi Barang Masuk dan Keluar
          <select class="form-select" id="inputGroupSelect01" style="float: right; clear: none; margin-bottom: 2%; margin-right: 0.5%; width: 30%;">
            <option selected style="float: right; clear: none; margin-top: 0.5%; margin-right: 0.5%;">Choose...</option>
            <option value="1" style="float: right; clear: none; margin-top: 0.5%; margin-right: 0.5%;">One</option>
            <option value="2" style="float: right; clear: none; margin-top: 0.5%; margin-right: 0.5%;">Two</option>
            <option value="3" style="float: right; clear: none; margin-top: 0.5%; margin-right: 0.5%; width: 5px;">Three</option>
          </select>
  
    
        </p>
      </div>

      <table class="table table-hover table-striped"  >
        <thead class="table-primary">
          <tr>
            <th scope="col">Kode</th>
            <th scope="col">Nama</th>
            <th scope="col">Varian</th>
            <th scope="col">Satuan</th>
            <th scope="col">Jumlah</th>
            <th scope="col">Harga Barang</th>
            <th scope="col">Tanggal Masuk</th>
            <th scope="col">Tanggal Keluar</th>
            <th scope="col">Nama Supplier</th>
          </tr>
        </thead>
        <tbody>
          <% rows.forEach((item)=> { %>
            <tr>
              <th scope="row">
                <%= item.kode_barang %>
              </th>
              <td>
                <%= item.nama_barang %>
              </td>
              <td>
                <%= item.varian_barang %>
              </td>
              <td>
                <%= item.satuan_barang %>
              </td>
              <td>
                <%= item.jumlah_barang %>
              </td>
              <td>
                <%= item.harga_barang %>
              </td>
              <td>
                <%= moment(item.tanggal_masuk).format('DD MMMM YYYY') %>
              </td>
              <td>
                <%= moment(item.tanggal_keluar).format('DD MMMM YYYY') %>
              </td>
              <td>
                <%= item.nama_supplier %>
              </td>
            </tr>
            <% }) %>
        </tbody>
      </table>
      <div class="container" style="margin: 1px;">
        <p style="color: blue; height: 15px;">Data barang dengan jumlah stok
          <select class="form-select" id="inputGroupSelect01" style="float: right; clear: none; margin-bottom: 2%; margin-right: 0.5%; width: 30%;">
            <option selected style="float: right; clear: none; margin-top: 0.5%; margin-right: 0.5%;">Choose...</option>
            <option value="1" style="float: right; clear: none; margin-top: 0.5%; margin-right: 0.5%;">One</option>
            <option value="2" style="float: right; clear: none; margin-top: 0.5%; margin-right: 0.5%;">Two</option>
            <option value="3" style="float: right; clear: none; margin-top: 0.5%; margin-right: 0.5%; width: 5px;">Three</option>
          </select>
  
    
        </p>


      </div>

      <table class="table table-hover table-striped table-success">
        <thead class="table-info">
          <tr>
            <th scope="col">Kode</th>
            <th scope="col">Nama</th>
            <th scope="col">Varian</th>
            <th scope="col">Satuan</th>
            <th scope="col">Jumlah</th>
            <th scope="col">Harga Barang</th>
            <th scope="col">Tanggal Masuk</th>
            <th scope="col">Tanggal Keluar</th>
            <th scope="col">Nama Supplier</th>
          </tr>
        </thead>
        <tbody>
          <% rows.forEach((item)=> { %>
            <tr>
              <th scope="row">
                <%= item.kode_barang %>
              </th>
              <td>
                <%= item.nama_barang %>
              </td>
              <td>
                <%= item.varian_barang %>
              </td>
              <td>
                <%= item.satuan_barang %>
              </td>
              <td>
                <%= item.jumlah_barang %>
              </td>
              <td>
                <%= item.harga_barang %>
              </td>
              <td>
                <%= moment(item.tanggal_masuk).format('DD MMMM YYYY') %>
              </td>
              <td>
                <%= moment(item.tanggal_keluar).format('DD MMMM YYYY') %>
              </td>
              <td>
                <%= item.nama_supplier %>
              </td>
            </tr>
            <% }) %>
        </tbody>
      </table>
